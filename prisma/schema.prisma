// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model ServerValues {
  key String @id
  value String
}

model FrontPageMessage {
  id String @id
  message String
  createdAt DateTime @default(now())
}
